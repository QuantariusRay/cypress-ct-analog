{
  "name": "cypress-ct-analog",
  "version": "1.0.1",
  "description": "Framework Definition for AnalogJS with Cypress Component Testing",
  "exports": {
    "node": "./definition.js",
    "default": "./index.js"
  },
  "scripts": {
    "build": "npm run build:definition && npm run build:mount",
    "build:mount": "tsc --project tsconfig.mount.json",
    "build:definition": "tsc --project tsconfig.definition.json"
  },
  "types": "*.d.ts",
  "files": [
    "package.json",
    "definition.js",
    "index.js",
    "definition.d.ts",
    "index.d.ts"
  ],
  "devDependencies": {
    "cypress": "^12.7.0",
    "@angular/core": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@analogjs/platform": "^0.2.0-beta.16",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@cypress/mount-utils": "^4.0.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuantariusRay/cypress-ct-analog.git"
  },
  "keywords": [
    "cypress",
    "analog"
  ],
  "author": "Q Ray",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QuantariusRay/cypress-ct-analog/issues"
  },
  "homepage": "https://github.com/QuantariusRay/cypress-ct-analog#readme"
}
